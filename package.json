{
  "name": "easy-react-starter",
  "version": "0.0.1",
  "description": "This is very easy react starter.",
  "main": "index.js",
  "scripts": {
    "build:dev": "set NODE_ENV='development' && webpack  --debug --config config/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "clean:dist": "npm run rimraf -- dist",
    "server:dev": "set NODE_ENV='development' && webpack-dev-server --debug --hot --output-pathinfo --watch --colors --inline --content-base src --browser",
    "start": "npm run server:dev",
    "rimraf": "rimraf",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "set NODE_ENV=test && karma start ./karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudoki2015/easy-react-starter.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "es6",
    "babel",
    "webpack"
  ],
  "author": "Andrey Egorov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudoki2015/easy-react-starter/issues"
  },
  "homepage": "https://github.com/sudoki2015/easy-react-starter#readme",
  "dependencies": {
